{
  "name": "nuxt-vuetify-admin",
  "version": "0.0.0",
  "description": "Nuxt3 + Vuetify Admin template",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NI-Web-Infra-Team/nuxt-vuetify-admin"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "run-p nuxt-prepare generate-pkg-type",
    "nuxt-prepare": "nuxt prepare",
    "build": "run-s nuxt-build pm2-copy-to-output",
    "nuxt-build": "nuxt build",
    "dev": "run-s generate-pkg-type nuxt-dev",
    "nuxt-dev": "nuxt dev --port 13000 --dotenv .env.development",
    "upgrade-dep": "taze -w -i",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "icon-gen": "node ./scripts/generate-icons.cjs -i ./public/favicon.svg -o ./public/images -p favicon-",
    "lint": "vue-tsc --noEmit && eslint .",
    "lint-fix": "prettier . --write && eslint . --fix",
    "generate-api-type": "node ./scripts/generate-api-type.cjs -u https://petstore.swagger.io/v2/swagger.json -o ./types/__services__ -n PetStoreApi",
    "generate-pkg-type": "quicktype package.json -o ./types/__quicktype__/package.d.ts --just-types",
    "pm2-copy-to-output": "fse copy --errorOnExist ecosystem.config.cjs .output/ecosystem.config.cjs"
  },
  "dependencies": {
    "@trpc/client": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "echarts": "^5.4.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "trpc-nuxt": "^0.10.12",
    "tslib": "^2.6.2",
    "vue-echarts": "^6.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@mdi/js": "^7.3.67",
    "@mockoon/cli": "^4.1.0",
    "@nuxt/devtools": "^0.8.5",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.0.0-rc.5",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.2",
    "@pinia/nuxt": "^0.4.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.6",
    "@semantic-release/gitlab-config": "^12.0.1",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.4",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "autoprefixer": "^10.4.16",
    "commander": "^11.0.0",
    "daisyui": "^3.9.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsonc": "^2.10.0",
    "git-cz": "^4.9.0",
    "icon-gen": "^3.0.1",
    "lint-staged": "^15.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.7.4",
    "nuxt-icon": "^0.5.0",
    "octokit": "^3.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "quicktype": "^23.0.76",
    "sass": "^1.69.3",
    "sharp": "^0.32.6",
    "simple-git-hooks": "^2.9.0",
    "swagger-typescript-api": "^13.0.3",
    "tailwindcss": "^3.3.3",
    "taze": "^0.11.3",
    "terser": "^5.21.0",
    "type-fest": "^4.4.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.19",
    "vuetify": "^3.3.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  }
}
